{
  "name": "@jsume/validator-cli",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "packageManager": "pnpm@10.6.5",
  "description": "validate your jsume json file",
  "author": "Moozon <muzhenweizh@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jsume/validator-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsume/validator-cli.git"
  },
  "keywords": [
    "jsume",
    "validator",
    "cli"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "jsm-vldt": "bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@jsume/schema": "^0.0.6",
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@types/node": "^22.13.14",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "taze": "^19.0.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
